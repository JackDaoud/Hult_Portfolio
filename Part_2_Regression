   "source": [
    "# Consolidate predictions & deviations into a dataframe\n",
    "prediction_results = pd.DataFrame(data = {\n",
    "    'Original Sale Prices' : y_test,\n",
    "    'LR Predictions'       : ols_pred.round(decimals = 2),\n",
    "    'Lasso Predictions'    : lasso_pred.round(decimals = 2),\n",
    "    'ARD Predictions'      : ard_pred.round(decimals = 2),\n",
    "    'LR Deviation'         : ols_pred.round(decimals = 2) - y_test,\n",
    "    'Lasso Deviation'      : lasso_pred.round(decimals = 2) - y_test,\n",
    "    'ARD Deviation'        : ard_pred.round(decimals = 2) - y_test,\n",
    "    })\n",
    "\n",
    "# Export predictions\n",
    "prediction_results.to_excel(excel_writer = './_model_results/model_predictions.xlsx',\n",
    "                            index = False)\n",
    "\n",
    "#prediction_results"
   ]
